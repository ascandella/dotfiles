{
    "telemetry.enableTelemetry": false,
    "editor.cursorBlinking": "phase",
    "editor.tabSize": 2,
    "editor.roundedSelection": true,
    "editor.glyphMargin": true,
    "editor.lineNumbers": "off",
    "editor.rulers": [
        100
    ],
    "editor.renderIndentGuides": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.scrollBeyondLastLine": false,
    "editor.minimap.enabled": false,
    "files.eol": "\n",
    "files.exclude": {
        //"**/.tmp": true,
        ".venv": true,
        "**/vendor": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "go-build/.go": true,
        ".gen": true,
        ".vscode": true,
        "env": true,
        "env_docs": true,
        "udeploy": true,
        "debian": true,
        "**/*.test": true,
        "**/*.pyc": true,
        "Godeps/_workspace": true,
        "**/coverage.html": true,
        "**/_build": true,
        "**/*.coverprofile": true,
        "**/__pycache__": true
    },
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "go.autocompleteUnimportedPackages": true,
    "go.lintTool": "gometalinter",
    "go.lintFlags": [
        "--fast",
        "--enable=errcheck"
    ],
    "go.vetFlags": [
        "-printfuncs=wrapf,statusf,warnf,infof,debugf,noerrorf,equalf,newexiterrorf,containsf,truef,falsef,equalf,emptyf,failf"
    ],
    "explorer.confirmDelete": false,
    "window.zoomLevel": 0,
    "python.venvPath": "${workspaceFolder}",
    "extensions.ignoreRecommendations": false,
    "eslint.format.enable": true,
    "eslint.lintTask.enable": true,
    "eslint.packageManager": "yarn",
    "emmet.includeLanguages": {
        "html-eex": "html"
    },
    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.includeLanguages": {
        "typescriptreact": "html",
        "typescript": "javascript"
    },
    "workbench.colorTheme": "One Dark Pro",
    "editor.fontFamily": "Monaco, Menlo, Consolas, 'Courier New', monospace",
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": false,
    "python.linting.mypyEnabled": true,
    "vim.commandLineModeKeyBindingsNonRecursive": [
        {
            "before": [
                "W"
            ],
            "after": [
                "w"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-c>": false,
    },
    "terminal.integrated.fontFamily": "UbuntuMono NF",
    "terminal.integrated.fontSize": 16,
    "settingsSync.ignoredExtensions": [],
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "[typescriptreact]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "css.validate": false,
    "stylelint.enable": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {"before": [",", "a"], "commands": ["fzf-quick-open.runFzfSearch"]}
    ],
    "editor.multiCursorModifier": "alt",
}